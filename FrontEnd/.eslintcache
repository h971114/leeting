n    }\r\n}\r\n\r\nfunction App({startDates}) {\r\n    // 달력 날짜 변경 시 기준점이 되는 날짜 \r\n    const [startDate, setStartDate] = useState(new Date(startDates));\r\n            \r\n    // 요일 반환 \r\n    const getDayName = (date) => {\r\n        return date.toLocaleDateString('ko-KR', { weekday: 'long', }).substr(0, 1);\r\n    }\r\n    \r\n    // 날짜 비교시 년 월 일까지만 비교하게끔 \r\n    const createDate = (date) => {\r\n        return new Date(\r\n            new Date(\r\n                date.getFullYear(),\r\n                date.getMonth(),\r\n                date.getDate(),\r\n                0,\r\n                0,\r\n                0));\r\n    }\r\n    \r\n\r\n    return ( \r\n    <>\r\n            <DatePicker\r\n                id=\"datepick\"\r\n                locale=\"ko\" // 달력 한글화\r\n                dateFormat=\"yyyy-MM-dd\"\r\n                selected={startDate} // 날짜 state\r\n                onChange={setStartDate} // 날짜 설정 콜백 함수 \r\n                popperModifiers={{\r\n                    // 모바일 web 환경에서 화면을 벗어나지 않도록 하는 설정 \r\n                    preventOverflow: { enabled: true, },\r\n                }} popperPlacement=\"auto\" // 화면 중앙에 팝업이 뜨도록 \r\n                // 토요일, 일요일 색깔 바꾸기 위함\r\n                dayClassName={\r\n                    date => getDayName(createDate(date)) === '토' ? \"saturday\"\r\n                        :\r\n                        getDayName(createDate(date)) === '일' ? \"sunday\" : undefined\r\n                }\r\n            />\r\n        </>\r\n    );\r\n}\r\n\r\nexport default Modify;","C:\\Leeting_git\\Leeting\\FrontEnd\\src\\components\\meeting\\diy.js",[],"C:\\Leeting_git\\Leeting\\FrontEnd\\src\\components\\meeting\\my.js",[],"C:\\Leeting_git\\Leeting\\FrontEnd\\src\\components\\meeting\\music.js",[],"C:\\Leeting_git\\Leeting\\FrontEnd\\src\\components\\meeting\\lans.js",[],"C:\\Leeting_git\\Leeting\\FrontEnd\\src\\components\\meeting\\exercise.js",[],"C:\\Leeting_git\\Leeting\\FrontEnd\\src\\components\\meeting\\game.js",[],"C:\\Leet